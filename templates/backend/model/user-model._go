package model

// User is
type User struct {
	BaseModel
	Name     string `` //
	Email    string `` //
	Password string `` //
	Role     string `` //
}

// UserResponse is
type UserResponse struct {
	ID    string `json:"id"`    //
	Name  string `json:"name"`  //
	Email string `json:"email"` //
	Role  string `json:"role"`  //
}

// RegisterUserRequest is
type RegisterUserRequest struct {
	Name     string `json:"name"`     //
	Email    string `json:"email"`    //
	Password string `json:"password"` //
}

// LoginUserRequest is
type LoginUserRequest struct {
	Email    string `json:"email"`    //
	Password string `json:"password"` //
}
